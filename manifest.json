{
    "manifest_version": 3,
    "version": "1.0",
    "name": "News Generator",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": ["storage", "tabs"],
    "content_security_policy": 
    {
        "extension_pages": "default-src 'self'; style-src 'self' 'unsafe-inline'; style-src-elem 'self' 'unsafe-inline'"
     }
}